    <!-- 회원 관리 -->
    <sqlMap resource="sqlMap/Member.xml"/>	
    	
	<!-- eGovFramework file 첨부 관리-->
	<sqlMap resource="sqlMap/EgovFileManage.xml"/>
		

    <!-- 시/군구 조회 -->
    <sqlMap resource="sqlMap/SiGungu.xml"/>	
	
    
    <!-- 포인트 관리 -->
    <sqlMap resource="sqlMap/Point.xml"/>	
    
    
</sqlMapConfig>
